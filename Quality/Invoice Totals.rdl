    <DataSet Name="Param_Customer">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvStart">
            <Value>=Parameters!InvStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvEnd">
            <Value>=Parameters!InvEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
                             SELECT DISTINCT b.CustCode, c.CustName
                              FROM            dbo.PV_BillLine bl INNER JOIN
                                                       dbo.PV_Bill b ON bl.BillNum = b.BillNum INNER JOIN
                                                       dbo.PV_Customer c ON b.CustCode = c.CustCode
                              WHERE        bl.BillDate BETWEEN @InvStart AND @InvEnd
                              ORDER BY c.CustName</CommandText>
      </Query>
      <Fields>
        <Field Name="CustCode">
          <DataField>CustCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
