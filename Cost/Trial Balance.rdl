    <DataSet Name="Param_Year">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <CommandText>SELECT        NULL AS ParamValue, 'All' AS ParamLabel, 9999 AS ParamSort
UNION
SELECT DISTINCT YearNum, CAST(YearNum AS VARCHAR(4)) AS Expr1, YearNum AS Expr2
FROM            PV_XLGLPostings
ORDER BY ParamSort DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ParamValue">
          <DataField>ParamValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ParamLabel">
