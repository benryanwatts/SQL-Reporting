    <DataSet Name="Param_ItemType">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <CommandText>select distinct t.ItemTypeCode
		, t.ItemTypeCode+'-'+t.ItemTypeName
from dbo.PM_Item i
	INNER JOIN dbo.PV_ItemType t on i.ItemTypeCode=t.ItemTypeCode and i.ItemGroupCode=t.ItemGroupCode
where i.InActive &lt;&gt; 1
	and i.ReqGroupCode in ('10','5','70')</CommandText>
      </Query>
