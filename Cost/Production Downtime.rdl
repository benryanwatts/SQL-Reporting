    <DataSet Name="Param_Operator">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartTimeBegin">
            <Value>=Parameters!StartTimeBegin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartTimeEnd">
            <Value>=Parameters!StartTimeEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select null OpCode, 'All' OpName, 1 Sort
union all
select distinct e.[op-code], e.[op-code], 2
from LEN_SFDC.dbo.sfeventcds  e
	INNER JOIN LEN_SFDC.dbo.sfmachine m on e.[mach-id]=m.[mach-id] and e.kco=m.kco
	LEFT JOIN LEN_SFDC.dbo.sfstatcode sc on e.[stat-code]=sc.[stat-code] and e.kco=sc.kco
where convert(VARCHAR, DATEADD(S, e.[start-time], '1990-01-01'), 22) between @StartTimeBegin and @StartTimeEnd
order by Sort, OpCode</CommandText>
      </Query>
      <Fields>
        <Field Name="OpCode">
          <DataField>OpCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OpName">
          <DataField>OpName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sort">
          <DataField>Sort</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Param_Customer">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartTimeBegin">
            <Value>=Parameters!StartTimeBegin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartTimeEnd">
            <Value>=Parameters!StartTimeEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Operator">
            <Value>=Parameters!Operator.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Shift">
            <Value>=Parameters!Shift.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct c.CustCode, c.CustName
from LEN_SFDC.dbo.sfeventcds  e
	LEFT JOIN dbo.PV_Job j on e.kjobcode=j.JobCode
	LEFT JOIN dbo.PV_Customer c on j.CustCode=c.CustCode
where convert(VARCHAR, DATEADD(S, e.[start-time], '1990-01-01'), 22) between @StartTimeBegin and @StartTimeEnd
	and (e.[op-code] = @Operator or @Operator is null)
	and e.[stat-code] &gt; '0'
	and (e.[shift-code] = @Shift or @Shift is null)
order by c.CustName</CommandText>
      </Query>
      <Fields>
        <Field Name="CustCode">
          <DataField>CustCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustName">
          <DataField>CustName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Param_Shift">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <CommandText>select null shiftcode, 'All' Descriptions
union all
select s.[shift-code], s.description
from LEN_SFDC.dbo.sfShiftcode s
where s.kco = 11</CommandText>
      </Query>
      <Fields>
        <Field Name="shiftcode">
