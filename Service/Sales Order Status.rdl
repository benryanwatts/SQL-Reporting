    <DataSet Name="Param_Customer">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CSR">
            <Value>=Parameters!CSR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select null CustCode, 'All' CustName, 1 Sort
UNION ALL
select distinct c.CustCode, c.CustName, 2
from dbo.PV_SOrder s 
	INNER JOIN dbo.PV_Customer c on s.CustCode=c.CustCode
where s.SOrderStat not in (8,9)
		and s.CSRUserCode in (@CSR)</CommandText>
      </Query>
      <Fields>
        <Field Name="CustCode">
          <DataField>CustCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustName">
          <DataField>CustName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sort">
          <DataField>Sort</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Param_CSR">
      <Query>
        <DataSourceName>LEN_RADIUS</DataSourceName>
        <CommandText>select distinct CSRUserCode
from dbo.PV_SOrder
where CSRUserCode not in ('1001','Amandar','andresg','bobl','cherylf','csrtemp','jerryb','juliey','kameronf','marshab','michele','nadinel','SeanS','shawnf','Sherrym','terryl','Mikedd'
							, 'nadinel','SeanS','shawnf','Sherrym','terryl')
and CSRUserCode is not null
order by CSRUserCode</CommandText>
      </Query>
      <Fields>
        <Field Name="CSRUserCode">
          <DataField>CSRUserCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
